<div class=container>
  <h1>Businesses#index</h1>

  <p>Find me in app/views/businesses/index.html.erb</p>
  <table class="table">
    <thead class="thead-dark">
      <tr>
        <th>title</th>
      </tr>
    </thead>
    <% @businesses.each do |business| %>
    <tr>
      <td><%= business.title %></td>
      <td><%= link_to "show", business_path(business) %></td>
      <td><%= link_to "edit", edit_business_path(business) %></td>
      <td><%= link_to "delete", business_path(business), method: :delete, data: {confirm:"Are you sure?"}%>
      </td>
      <% end %>
    </tr>
  </table>
  <div class="btn btn-info">
    <%= link_to "create new ", new_business_path, :class => "btn" %>
  </div>
</div>
